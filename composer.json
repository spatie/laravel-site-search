{
    "name": "spatie/laravel-site-search",
    "description": "A site search engine",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-site-search",
        "crawl",
        "search"
    ],
    "homepage": "https://github.com/spatie/laravel-site-search",
    "license": "MIT",
    "authors": [
        {
            "name": "freek",
            "email": "freek@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "guzzlehttp/psr7": "^2.8",
        "illuminate/contracts": "^12.0|^13.0",
        "illuminate/http": "^12.0|^13.0",
        "laravel/prompts": "^0.1.23|^0.3.13",
        "spatie/crawler": "^9.0",
        "spatie/laravel-package-tools": "^1.93"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.10",
        "meilisearch/meilisearch-php": "^1.16.1",
        "nunomaduro/collision": "^8.9.1",
        "orchestra/testbench": "^10.9|^11.0",
        "pestphp/pest": "^4.4.1",
        "pestphp/pest-plugin-laravel": "^4.1",
        "larastan/larastan": "^3.0",
        "spatie/laravel-ray": "^1.43.6"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Spatie\\SiteSearch\\": "src",
            "Spatie\\SiteSearch\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "suggest": {
        "meilisearch/meilisearch-php": "Required to use the MeiliSearch engine."
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-sqlite": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Spatie\\SiteSearch\\SiteSearchServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
